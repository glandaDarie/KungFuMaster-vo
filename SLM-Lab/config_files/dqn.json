{
    "dqn_KungFuMaster": {
      "agent": [{
        "name": "DoubleDQN",
        "algorithm": {
          "name": "DoubleDQN",
          "action_pdtype": "Argmax",
          "action_policy": "epsilon_greedy",
          "explore_var_spec": {
            "name": "linear_decay",
            "start_val": 5.0,
            "end_val": 0.5,
            "start_step": 0,
            "end_step": 5000
          },
          "gamma": 0.99,
          "training_batch_iter": 8,
          "training_iter": 4,
          "training_frequency": 4,
          "training_start_step": 32
        },
        "memory": {
          "name": "Replay",
          "batch_size": 32,
          "max_size": 200000,
          "use_cer": false
        },
        "net": {
          "type": "ConvNet",
          "conv_hid_layers": [
            [16, 8, 4, 0, 1],
            [32, 4, 2, 0, 1],
            [16, 3, 1, 0, 1]
          ],
          "fc_hid_layers": [64],
          "hid_layers_activation": "relu",
          "init_fn": "orthogonal_",
          "normalize": true,
          "batch_norm": false,
          "clip_grad_val": 0.5,
          "loss_spec": {
            "name": "MSELoss"
          },
          "optim_spec": {
            "name": "RMSprop",
            "lr": 1e-4
          },
          "lr_scheduler_spec": null,
          "update_type": "replace",
          "update_frequency": 1000,
          "gpu": true
        }
      }],
      "env": [{
        "name": "KungFuMaster-v0",
        "max_t": null,
        "frame_op": "concat",
        "reward_scale": "sign",
        "frame_op_len": 4,
        "num_envs": 16,
        "max_frame": 750000
      }],
      "body": {
        "product": "outer",
        "num": 1
      },
      "meta": {
        "distributed": false,
        "eval_frequency": 100000,
        "log_frequency": 100000,
        "max_trial": 1,
        "max_session": 1,
        "rigorous_eval": 0
      }
    }
  }
