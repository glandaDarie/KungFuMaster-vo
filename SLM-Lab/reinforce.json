{
    "reinforce_KungFuMaster": {
      "agent": [
        {
          "name": "Reinforce",
          "algorithm": {
            "name": "Reinforce",
            "action_pdtype": "default",
            "action_policy": "default",
            "center_return": true,
            "explore_var_spec": null,
            "gamma": 0.99,
            "entropy_coef_spec": {
              "name": "linear_decay",
              "start_val": 0.01,
              "end_val": 0.001,
              "start_step": 0,
              "end_step": 20000
            },
            "training_frequency": 1
          },
          "memory": {
            "name": "OnPolicyReplay"
          },
          "net": {
            "type": "ConvNet",
            "shared": false,
            "conv_hid_layers": [
              [64, 8, 4, 0, 1],
              [128, 4, 2, 0, 1],
              [64, 3, 1, 0, 1]
            ],
            "fc_hid_layers": [128, 256],
            "hid_layers_activation": "relu",
            "normalize": true,
            "init_fn": "xavier_uniform_",
            "clip_grad_val": 1.0,
            "loss_spec": {
              "name": "MSELoss"
            },
            "optim_spec": {
              "name": "Adam",
              "lr": 0.1
            },
            "lr_scheduler_spec": null,
            "gpu": true,
            "update_frequency": 1
          }
        }
      ],
      "env": [
        {
          "name": "KungFuMaster-v0",
          "max_t": null,
          "max_frame": 1000000
        }
      ],
      "body": {
        "product": "outer",
        "num": 1
      },
      "meta": {
        "distributed": "shared",
        "rigorous_eval": 0,
        "max_session": 1,
        "max_trial": 1,
        "score_logging": true,
        "eval_frequency": 50,
        "log_frequency": 50
      },
      "search": {}
    }
  }
  